<?xml version="1.0" encoding="UTF-8"?>
<project name="module_instasent-java-lib" default="compile.module.instasent-java-lib">
  <dirname property="module.instasent-java-lib.basedir" file="${ant.file.module_instasent-java-lib}"/>
  
  <property name="module.jdk.home.instasent-java-lib" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.instasent-java-lib" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.instasent-java-lib" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.instasent-java-lib" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="instasent-java-lib.output.dir" value="${module.instasent-java-lib.basedir}/../out/production/instasent-java-lib"/>
  <property name="instasent-java-lib.testoutput.dir" value="${module.instasent-java-lib.basedir}/../out/test/instasent-java-lib"/>
  
  <path id="instasent-java-lib.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="instasent-java-lib.module.production.classpath">
    <path refid="${module.jdk.classpath.instasent-java-lib}"/>
    <pathelement location="${basedir}/../../Descargas/okio-1.6.0.jar"/>
    <pathelement location="${basedir}/../../Descargas/okhttp-3.3.0.jar"/>
    <pathelement location="${basedir}/../../Descargas/json-simple-1.1.1.jar"/>
  </path>
  
  <path id="instasent-java-lib.runtime.production.module.classpath">
    <pathelement location="${instasent-java-lib.output.dir}"/>
    <pathelement location="${basedir}/../../Descargas/okio-1.6.0.jar"/>
    <pathelement location="${basedir}/../../Descargas/okhttp-3.3.0.jar"/>
    <pathelement location="${basedir}/../../Descargas/json-simple-1.1.1.jar"/>
  </path>
  
  <path id="instasent-java-lib.module.classpath">
    <path refid="${module.jdk.classpath.instasent-java-lib}"/>
    <pathelement location="${instasent-java-lib.output.dir}"/>
    <pathelement location="${basedir}/../../Descargas/okio-1.6.0.jar"/>
    <pathelement location="${basedir}/../../Descargas/okhttp-3.3.0.jar"/>
    <pathelement location="${basedir}/../../Descargas/json-simple-1.1.1.jar"/>
  </path>
  
  <path id="instasent-java-lib.runtime.module.classpath">
    <pathelement location="${instasent-java-lib.testoutput.dir}"/>
    <pathelement location="${instasent-java-lib.output.dir}"/>
    <pathelement location="${basedir}/../../Descargas/okio-1.6.0.jar"/>
    <pathelement location="${basedir}/../../Descargas/okhttp-3.3.0.jar"/>
    <pathelement location="${basedir}/../../Descargas/json-simple-1.1.1.jar"/>
  </path>
  
  
  <patternset id="excluded.from.module.instasent-java-lib">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.instasent-java-lib">
    <patternset refid="excluded.from.module.instasent-java-lib"/>
  </patternset>
  
  <path id="instasent-java-lib.module.sourcepath">
    <dirset dir="${module.instasent-java-lib.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.instasent-java-lib" depends="compile.module.instasent-java-lib.production,compile.module.instasent-java-lib.tests" description="Compile module instasent-java-lib"/>
  
  <target name="compile.module.instasent-java-lib.production" depends="register.custom.compilers" description="Compile module instasent-java-lib; production classes">
    <mkdir dir="${instasent-java-lib.output.dir}"/>
    <javac2 destdir="${instasent-java-lib.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.instasent-java-lib}/javac">
      <compilerarg line="${compiler.args.instasent-java-lib}"/>
      <bootclasspath refid="instasent-java-lib.module.bootclasspath"/>
      <classpath refid="instasent-java-lib.module.production.classpath"/>
      <src refid="instasent-java-lib.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.instasent-java-lib"/>
    </javac2>
    
    <copy todir="${instasent-java-lib.output.dir}">
      <fileset dir="${module.instasent-java-lib.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.instasent-java-lib.tests" depends="register.custom.compilers,compile.module.instasent-java-lib.production" description="compile module instasent-java-lib; test classes" unless="skip.tests"/>
  
  <target name="clean.module.instasent-java-lib" description="cleanup module">
    <delete dir="${instasent-java-lib.output.dir}"/>
    <delete dir="${instasent-java-lib.testoutput.dir}"/>
  </target>
</project>